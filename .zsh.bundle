zplugin ice lucid wait'!0'; zplugin light "voronkovich/gitignore.plugin.zsh"
ZNOTIFY_BLACKLIST="find|git|vi|ssh|ag|less"
zplugin ice lucid wait'!0' atload"zstyle ':notify:*' blacklist-regex '$ZNOTIFY_BLACKLIST'"; zplugin light "marzocchi/zsh-notify"
zplugin ice lucid wait'!0'; zplugin light "knu/zsh-git-escape-magic"
export BASE16_SHELL_HOOKS=$HOME/.config/base16/hooks
zplugin ice lucid; zplugin light "chriskempson/base16-shell"
zplugin ice lucid wait'!0'; zplugin light "gimbo/venv-lite.zsh"
zplugin ice lucid wait'!0'; zplugin light "chrissicool/zsh-256color"
