[user]
	name = Benjamin Stier
	email = b.stier@levigo.de
[include]
    path = .gitconfig.global

# vim: ft=gitconfig
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[credential]
	helper = store
[commit]
	gpgsign = true
[gpg]
	program = /usr/local/bin/gpg
